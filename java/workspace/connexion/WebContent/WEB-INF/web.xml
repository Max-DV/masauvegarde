<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>connexion</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>

	</welcome-file-list>



	<servlet>
		<servlet-name>Bam</servlet-name>
		<servlet-class>ServletConnexion.ServletIndex</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Bam</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Bum</servlet-name>
		<servlet-class>ServletConnexion.ServletConnexion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Bum</servlet-name>
		<url-pattern>/connexion2.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Bim</servlet-name>
		<servlet-class>ServletConnexion.ServletDeconnexion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Bim</servlet-name>
		<url-pattern>/deconnexion.html</url-pattern>
	</servlet-mapping>

</web-app>